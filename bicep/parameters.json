{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "webAppName": {
      "value": "buddy-agent-webapp"
    },
    "location": {
      "value": "eastus"
    },
    "appServicePlanSku": {
      "value": "B1"
    },
    "dotnetVersion": {
      "value": "v9.0"
    },
    "sqlAdminLogin": {
      "value": "buddyagentadmin"
    },
    "sqlAdminPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<subscription-id>/resourceGroups/<resource-group>/providers/Microsoft.KeyVault/vaults/<vault-name>"
        },
        "secretName": "SqlAdminPassword"
      }
    },
    "sqlServerName": {
      "value": "buddy-agent-sql"
    },
    "sqlDatabaseName": {
      "value": "BuddyAgentDb"
    },
    "sqlDatabaseSku": {
      "value": "Basic"
    }
  }
}
